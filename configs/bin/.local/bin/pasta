#!/usr/bin/env bash

set -euo pipefail

if command -v pbpaste &>/dev/null; then
  exec pbpaste
elif command -v wl-paste &>/dev/null; then
  exec wl-paste
elif command -v xclip &>/dev/null; then
  exec xclip -selection clipboard -o
elif [[ -e /tmp/clipboard ]]; then
  exec cat /tmp/clipboard
else
  echo ""
fi
